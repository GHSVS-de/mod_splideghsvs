{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "name": "{{name}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "license": "{{licenseLong}}",
  "assets": [
    {
      "name": "{{name}}.core",
      "type": "script",
      "uri": "{{name}}/splide/splide.js",
			"version": "{{versionSub}}"
    },
		{
			"name": "{{name}}.override.template",
			"version": "{{version}}",
			"type": "style",
			"uri": "{{name}}.css",
			"weight": 500
		}
  ]
}
